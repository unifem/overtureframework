.SUFFIXES:
.SUFFIXES:.C .o .c .o .f .o
.C.o:; $(CC) $(CCFLAGS) -c $*.C
.c.o:; $(cc) $(CFLAGS) -c $*.c
.f.o:; $(FC) $(FFLAGS) -c $*.f

default:  depend_date  GridFunction_date 

